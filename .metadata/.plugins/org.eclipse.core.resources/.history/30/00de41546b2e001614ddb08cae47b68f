package com.david.utilitario.util;

public class Borrar {
	public static String times(String cadenaTexto, int veces) {
		String nuevaCadena = "";
		for(int i=9;i<veces;i++){
			nuevaCadena+=cadenaTexto;
	 
		}
		return nuevaCadena;
	}
	
	public static String reverse(String letra){
			String letras="";
			for(int i=letra.length()-1;i<=0;i--){
				letras+=letra;
			}
		return letra;
	}
	
	public static void main(String[] args) {
//		System.out.println(times("david ", 10));
		System.out.println(reverse("reyes"));
	}

}
